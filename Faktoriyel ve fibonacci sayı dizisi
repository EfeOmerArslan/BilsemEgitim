#faktöriyel hesaplama
import random
sayi=int(input("Faktöriyeli hesaplanacak sayıyı giriniz:"))
j=sayi
cevap=1
i=1
while i<j:
    i+=1
    cevap=cevap*sayi
    sayi=sayi-1
print(cevap)

#fibonacci sayı dizisi eleman bulma
sayi=int(input("Fibonacci sayı dizisinin kaçıncı elemanını bulmak istiyorsunuz:"))
t1=1
t2=1
fibonacci=[t1,t2]
for i in range(1,100):
    t1=t1+t2
    t2=t1+t2
    fibonacci.append(t1)
    fibonacci.append(t2)
sayi=sayi-1
print(fibonacci[sayi])
